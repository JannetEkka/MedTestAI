# MedTestAI Environment Configuration
# UPDATED for Gemini 2.5 Flash Integration

# =====================================
# CRITICAL: Gemini API Configuration
# =====================================
# Get your API key from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=

# Optional Gemini Configuration
GEMINI_MODEL=gemini-1.5-flash
GEMINI_TEMPERATURE=0.7
GEMINI_MAX_TOKENS=4096

# =====================================
# Application Configuration
# =====================================
NODE_ENV=development
PORT=3001
FRONTEND_URL=https://medtestai-backend-1067292712875.us-central1.run.app

# =====================================
# Google Cloud Project Configuration
# =====================================
GOOGLE_CLOUD_PROJECT=
GOOGLE_CLOUD_REGION=us-central1

# =====================================
# Healthcare API Configuration (Optional)
# =====================================
HEALTHCARE_DATASET_ID=
HEALTHCARE_LOCATION=us-central1
FHIR_STORE_ID=

# =====================================
# Document AI Configuration (Optional)
# =====================================
DOCUMENT_AI_LOCATION=us
DOCUMENT_AI_PROCESSOR_ID=your_processor_id_here

# =====================================
# Security Configuration
# =====================================
JWT_SECRET=your_jwt_secret_here
SESSION_TIMEOUT=28800
MAX_FILE_SIZE=10485760

# =====================================
# Rate Limiting Configuration
# =====================================
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# =====================================
# Logging Configuration
# =====================================
LOG_LEVEL=info
AUDIT_LOG_ENABLED=true

# =====================================
# CORS Configuration
# =====================================
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# =====================================
# Database Configuration (Optional)
# =====================================
# DATABASE_URL=your_database_url_here

# =====================================
# External Integrations (Optional)
# =====================================
# JIRA_BASE_URL=https://your-domain.atlassian.net
# JIRA_PROJECT_KEY=MEDTEST
# TESTRAIL_URL=https://your-domain.testrail.io

# =====================================
# Development Tools
# =====================================
ENABLE_DEBUG_LOGGING=true
MOCK_MODE_ENABLED=false